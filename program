#include<stdio.h>
main(){
	int i,j;
	int arr[2][2]={1,2,3,4};
	int brr[2][2]={5,6,7,8};
	int sum[2][2];
	printf("Elements of array 1:\n");
	for(i=0;i<2;i++){
		for(j=0;j<2;j++){
			printf("%d\t",arr[i][j]);
		}
		printf("\n");
	}
	printf("Elements of array 2:\n");
	for(i=0;i<2;i++){
		for(j=0;j<2;j++){
			printf("%d\t",brr[i][j]);
		}
		printf("\n");
	}
	printf("The sum of array 1 and array 2 is:\n");
	for(i=0;i<2;i++){
		for(j=0;j<2;j++){
			sum[i][j]=arr[i][j]+brr[i][j];
	printf("%d\t",sum[i][j]);
		}
		printf("\n");
	}

}
